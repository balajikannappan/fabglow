package com.niit.middleend.dao;

import java.util.List;


import com.niit.backend.model.SupplierInfo;

public interface SupplierDao {
	public boolean addSupplier(SupplierInfo supplierInfo);
	public boolean updateSupplier(SupplierInfo supplierInfo);
	public boolean deleteSupplier(SupplierInfo supplierInfo);
	public SupplierInfo getSupplier(int supplierId);
	public List<SupplierInfo> listCategories();
}
